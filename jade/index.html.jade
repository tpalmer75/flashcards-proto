								//- 	span All Courses
								//- 	i.fa.fa-caret-down
								input(type="search" placeholder="Search for a deck...")
								button.blue-btn(type="submit")
									i.fa.fa-search
		section.preview
			.wrapper
				header.cf
					.deck-info
						div
							h3 AHTG 100
							.rating
								i.fa.fa-star
								i.fa.fa-star
								i.fa.fa-star
								i.fa.fa-star-half-full
								i.fa.fa-star-o
						h2 Midterm 1 Study Guide
					.controls
						a(href="#").control-btn.blue.long(style="margin-left: 0")
							.icon
								i.fa.fa-line-chart
							span STUDY
						a(href="#").control-btn.blue.edit-mode
							.icon
								i.fa.fa-edit
							span EDIT
						a(href="#").control-btn.blue
							.icon
								i.fa.fa-share
